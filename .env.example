# CARTEL47 Backend Environment Variables
# Copy this file to .env and fill in the actual values

# Node Environment
NODE_ENV=production

# Server Configuration
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/cartel47_db

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_change_in_production

# API Configuration
CORS_ORIGIN=http://localhost:3000,https://cartel47-frontend.vercel.app

# Blockchain/Web3 (Future)
ETHERUM_RPC_URL=https://polygon-testnet-rpc.url
WALLET_ADDRESS=0x
